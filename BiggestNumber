package ccom.corejava.seleniumcore;

public class BiggestNumber 
{
	public void m1()
	{
		System.out.println("method1 is executed");
	}
	public static void main(String[] args)
	{
		BiggestNumber num =new BiggestNumber();
		num.m1();
		int x=10;
		int y=20;
		if(x>y)
		{
			System.out.println("x is big:"+x);
			
		}
		else
		{
			System.out.println("y is big:"+y);
		}
	}

}
